{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "cdnjs",
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation@1.20.0",
      "destination": "wwwroot/lib/jquery-validation/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "admin-lte@3.2.0",
      "destination": "wwwroot/plugins/admin-lte/",
      "files": [
        "js/adminlte.min.js",
        "css/adminlte.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "icheck-bootstrap@3.0.1",
      "destination": "wwwroot/plugins/icheck-bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "axios@1.6.3",
      "destination": "wwwroot/lib/axios/",
      "files": [
        "axios.min.js",
        "axios.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "vue@3.4.3",
      "destination": "wwwroot/lib/vue/"
    }
  ]
}